DELETE FROM RT_FIELD_VALUES WHERE FIELD_ROW_ID = 
(SELECT field_row_id FROM RT_FIELD_VALUES WHERE REF_TABLE_FIELD_ID IN
(SELECT REF_TABLE_FIELD_ID FROM RT_TABLE_FIELD WHERE REF_TABLE_ID = 
(SELECT REF_TABLE_ID FROM RT_TABLE WHERE NAME='$ref_table_name')) 
and FIELD_VALUE IN ('$code_value_to_be_deleted' ));